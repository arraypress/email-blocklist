{
  "name": "arraypress/email-blocklist",
  "description": "A simple, efficient library for checking email addresses against disposable email provider lists.",
  "version": "1.0.0",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/email-blocklist",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "keywords": [
    "email",
    "blocklist",
    "disposable",
    "validation",
    "spam"
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\EmailBlocklist\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ArrayPress\\EmailBlocklist\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/email-blocklist/issues",
    "source": "https://github.com/arraypress/email-blocklist"
  }
}